# Snowflake Connection Configuration
# Copy this file to .env and fill in your actual credentials

# Account identifier (from your Snowflake URL)
# Example: abc12345.us-east-1 or mycompany
SNOWFLAKE_ACCOUNT=your_account_identifier

# Your Snowflake username (often your email)
SNOWFLAKE_USER=your_username

# Authentication Method:
# Option 1: For SSO (recommended for company accounts)
SNOWFLAKE_AUTHENTICATOR=externalbrowser
# Option 2: For username/password (comment out line above and uncomment below)
# SNOWFLAKE_PASSWORD=your_password

# Warehouse to use for compute
SNOWFLAKE_WAREHOUSE=COMPUTE_WH

# Database and Schema
SNOWFLAKE_DATABASE=PRODDB
SNOWFLAKE_SCHEMA=PUBLIC

# Your role (check with your admin if unsure)
SNOWFLAKE_ROLE=your_role
